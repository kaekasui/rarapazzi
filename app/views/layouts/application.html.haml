    %title paparazzi
  %body
    %nav.navbar.navbar-inverse
      .navbar-header
        %button.navbar-toggle.cpllapse{"data-target" => "#collapse-header", "data-toggle" => "collapse", :type => "button"}
          %span.sr-only Toggle navigation
          %span.icon-bar
          %span.icon-bar
          %span.icon-bar
        =link_to 'PAPARAZZI', example_index_path, class: "navbar-brand"
      #collapse-header.collapse.navbar-collapse
        %ul.nav.navbar-nav.navbar-right
          %li
            = link_to 'About', about_index_path
          %li
            = link_to 'ブランド一覧', brands_path
          %li
            = link_to 'お問い合わせ', new_contact_path
          %li
            = link_to '作成する', new_brand_path

    =yield

    .footer
      %center
        %p paparazzi all right reserved by risako